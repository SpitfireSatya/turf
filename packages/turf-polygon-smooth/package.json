    "test:tape": "node -r esm test.js",
    "test:types": "tsc --noEmit types.ts"
  },
  "devDependencies": {
    "benchmark": "*",
    "glob": "*",
    "load-json-file": "*",
    "rollup": "*",
    "tape": "*",
    "write-json-file": "*"
  },
  "dependencies": {
    "@turf/helpers": "6.x",
    "@turf/meta": "6.x"
  }
