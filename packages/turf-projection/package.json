{
  "name": "@turf/projection",
  "version": "6.0.1",
  "description": "turf projection module",
  "author": "Turf Authors",
  "contributors": [
    "Stefano Borghi <@stebogit>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf/issues"
  },
  "homepage": "https://github.com/Turfjs/turf",
  "repository": {
